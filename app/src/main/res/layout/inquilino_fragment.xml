<?xml version="1.0" encoding="utf-8"?>

<androidx.core.widget.NestedScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.inquilinos.InquilinoFragment"
    android:fillViewport="true"
    android:background="@drawable/fondo_degradado">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
            android:id="@+id/textView13"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="5dp"
            android:text="Teléfono Garante"
            android:textColor="#fFFFFFFf"
            android:textSize="18sp"
            app:layout_constraintBaseline_toBaselineOf="@+id/textView12"
            app:layout_constraintStart_toEndOf="@id/guideline4" />

        <TextView
            android:id="@+id/tvTelGarante"
            android:layout_width="190dp"
            android:layout_height="30dp"
            android:layout_marginStart="5dp"
            android:textColor="#9C27B0"
            android:textSize="18sp"
            app:layout_constraintBaseline_toBaselineOf="@+id/tvDni"
            app:layout_constraintStart_toEndOf="@id/guideline4"
            tools:ignore="SpeakableTextPresentCheck"/>

        <TextView
            android:id="@+id/tvApellido"
            android:layout_width="190dp"
            android:layout_height="30dp"
            android:layout_marginStart="16sp"
            android:textColor="#9C27B0"
            android:textSize="18sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toEndOf="@id/guideline3"
            app:layout_constraintTop_toBottomOf="@+id/textView11"
            app:layout_constraintVertical_bias="0.0"
            tools:ignore="SpeakableTextPresentCheck" />

        <TextView
            android:id="@+id/tvDni"
            android:layout_width="190dp"
            android:layout_height="30dp"
            android:layout_marginStart="16dp"
            android:textColor="#9C27B0"
            android:textSize="18sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="@+id/guideline3"
            app:layout_constraintTop_toBottomOf="@+id/textView12"
            app:layout_constraintVertical_bias="0.0" />

        <TextView
            android:id="@+id/tvTitEmail"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:text="E-mail"
            android:textColor="#fFFFFFFf"
            android:textSize="18sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toEndOf="@id/guideline3"
            app:layout_constraintTop_toBottomOf="@+id/tvDni"
            app:layout_constraintVertical_bias="0.056" />

        <TextView
            android:id="@+id/tvEmail"
            android:layout_width="190dp"
            android:layout_height="30dp"
            android:layout_marginStart="16dp"
            android:textColor="#9C27B0"
            android:textSize="18sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="@+id/guideline3"
            app:layout_constraintTop_toBottomOf="@+id/tvTitEmail"
            app:layout_constraintVertical_bias="0.0" />

        <TextView
            android:id="@+id/tvTitTelefono"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:text="Teléfono"
            android:textColor="#fFFFFFFf"
            android:textSize="18sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toEndOf="@id/guideline3"
            app:layout_constraintTop_toBottomOf="@+id/tvEmail"
            app:layout_constraintVertical_bias="0.099" />

        <TextView
            android:id="@+id/tvTelefono"
            android:layout_width="190dp"
            android:layout_height="30dp"
            android:layout_marginStart="16dp"
            android:textColor="#9C27B0"
            android:textSize="18sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="@+id/guideline3"
            app:layout_constraintTop_toBottomOf="@+id/tvTitTelefono"
            app:layout_constraintVertical_bias="0.0" />

        <TextView
            android:id="@+id/tvTitLugarTrabajo"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:text="Lugar de Trabajo"
            android:textColor="#fFFFFFFf"
            android:textSize="18sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toEndOf="@id/guideline3"
            app:layout_constraintTop_toBottomOf="@+id/tvTelefono"
            app:layout_constraintVertical_bias="0.099" />

        <TextView
            android:id="@+id/tvLugarTrabajo"
            android:layout_width="190dp"
            android:layout_height="30dp"
            android:layout_marginStart="16dp"
            android:textColor="#9C27B0"
            android:textSize="18sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="@+id/guideline3"
            app:layout_constraintTop_toBottomOf="@+id/tvTitLugarTrabajo"
            app:layout_constraintVertical_bias="0.0" />

        <TextView
            android:id="@+id/tvId"
            android:layout_width="190dp"
            android:layout_height="33dp"
            android:layout_marginStart="16dp"
            android:textColor="#9C27B0"
            android:textSize="18sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toEndOf="@id/guideline3"
            app:layout_constraintTop_toBottomOf="@+id/textView2"
            app:layout_constraintVertical_bias="0.0"
            tools:layout_constraintStart_toEndOf="@id/guideline3" />

        <TextView
            android:id="@+id/textView11"
            android:layout_width="150dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:text="Apellido"
            android:textColor="#fFFFFFFf"

            android:textSize="18sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toEndOf="@id/guideline3"
            app:layout_constraintTop_toBottomOf="@+id/tvNombre"
            app:layout_constraintVertical_bias="0.048" />

        <TextView
            android:id="@+id/tvNombre"
            android:layout_width="190dp"
            android:layout_height="30dp"
            android:layout_marginStart="16dp"
            android:textColor="#9C27B0"
            android:textSize="18sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toEndOf="@id/guideline3"
            app:layout_constraintTop_toBottomOf="@+id/textView6"
            app:layout_constraintVertical_bias="0.0" />

        <TextView
            android:id="@+id/textView12"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:text="DNI"
            android:textColor="#fFFFFFFf"
            android:textSize="18sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toEndOf="@id/guideline3"
            app:layout_constraintTop_toBottomOf="@+id/tvApellido"
            app:layout_constraintVertical_bias="0.032" />

        <TextView
            android:id="@+id/textView6"
            android:layout_width="150dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:text="Nombre"

            android:textColor="#fFFFFFFf"
            android:textSize="18sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toEndOf="@id/guideline3"
            app:layout_constraintTop_toBottomOf="@+id/tvId"
            app:layout_constraintVertical_bias="0.03" />

        <TextView
            android:id="@+id/textView2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:text="Código del Inquilino"
            android:textColor="#fFFFFFFf"
            android:textSize="18sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toEndOf="@id/guideline3"
            app:layout_constraintTop_toBottomOf="@+id/textView28"
            app:layout_constraintVertical_bias="0.1" />

        <TextView
            android:id="@+id/textView28"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Información del inquilino"
            android:textColor="#9C27B0"
            android:textSize="24dp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.496"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.05" />

        <TextView
            android:id="@+id/textView14"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="5dp"
            android:text="Garante"
            android:textColor="#fFFFFFFf"
            android:textSize="18sp"
            app:layout_constraintBaseline_toBaselineOf="@+id/textView11"
            app:layout_constraintStart_toEndOf="@id/guideline4" />

        <TextView
            android:id="@+id/tvGarante"
            android:layout_width="190dp"
            android:layout_height="30dp"
            android:layout_marginStart="5dp"
            android:textColor="#9C27B0"
            android:textSize="18sp"
            app:layout_constraintBaseline_toBaselineOf="@+id/tvApellido"
            app:layout_constraintStart_toEndOf="@id/guideline4"
            tools:ignore="SpeakableTextPresentCheck"/>


        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.0" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.5" />
    </androidx.constraintlayout.widget.ConstraintLayout>



</androidx.core.widget.NestedScrollView>